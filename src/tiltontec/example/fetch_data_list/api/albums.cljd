(ns tiltontec.example.fetch-data-list.api.albums
  (:require
    ["package:http/http.dart" :as http]))

(def ^:private base-url "https://jsonplaceholder.typicode.com/")

(defn get-album
  ([] (http/get (Uri.parse (str base-url "albums"))))
  ([page] (http/get (Uri.parse (str base-url "albums/" page)))))